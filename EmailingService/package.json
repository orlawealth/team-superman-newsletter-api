{
  "name": "superman-newsletter",
  "version": "1.0.0",
  "description": "A dockerized micro-service for subscribing for and sending newsletters",
  "main": "index.js",
  "author": "Team Superman",
  "engines": {
    "node": "12.16.x",
    "yarn": ">=1.15.2 || 1.22.x"
  },
  "scripts": {
    "build": "rm -rf ./src/build && babel ./src/. --out-dir ./src/build --copy-files",
    "start": "node ./src/build/index.js",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "docker:start": "yarn run db-migrate:dev && yarn run start:dev"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@hapi/joi": "^17.1.1",
    "babel-watch": "^7.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.10",
    "nodemailer": "^6.4.6",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1"
  }
}
